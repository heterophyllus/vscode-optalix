{
    "name": "vscode-optalix",
    "displayName": "OpTaliX",
    "description": "OpTaliX macro language support for Visual Studio Code",
    "publisher": "heterophyllus",
    "version": "0.2.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "OpTaliX"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/heterophyllus/vscode-optalix.git"
    },
    "icon": "images/icon.png",
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "optalix",
                "aliases": [
                    "optalix",
                    "OpTaliX"
                ],
                "extensions": [
                    "mac",
                    "MAC"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "optalix",
                "scopeName": "source.optalix",
                "path": "./syntaxes/optalix.tmLanguage.json"
            }
        ]
    }
}
